package edu.iut.gui.listeners;

import javax.swing.JOptionPane;

public class ApplicationErrorMessageDialog extends
		AbstractApplicationMessageDialog {

	@Override
	public void showMessage(String level, String message) {
		// TODO Auto-generated method stub
		/** UTILISER l'AIDE FORUNIR DANS L'ENNONCE */
		//Boîte du message d'erreur
		JOptionPane jop = new JOptionPane();
		jop.showMessageDialog(null, level, message, JOptionPane.ERROR_MESSAGE);
		
	}






}
